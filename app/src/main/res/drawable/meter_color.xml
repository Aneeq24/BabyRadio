<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/seekbarcolor" />
            <corners
                android:bottomLeftRadius="@dimen/corner_seekbar"
                android:bottomRightRadius="@dimen/corner_seekbar"
                android:topLeftRadius="@dimen/corner_seekbar"
                android:topRightRadius="@dimen/corner_seekbar"></corners>

        </shape>
    </item>
    <item>
        <clip>
            <shape>
                <gradient
                    android:centerX="0.5"
                    android:endColor="@color/colorRed"
                    android:startColor="@color/colorGreen" />
                <corners
                    android:bottomLeftRadius="@dimen/corner_seekbar"
                    android:bottomRightRadius="@dimen/corner_seekbar"
                    android:centerX="0.75"
                    android:topLeftRadius="@dimen/corner_seekbar"
                    android:topRightRadius="@dimen/corner_seekbar"></corners>
            </shape>
        </clip>
    </item>
</layer-list>
